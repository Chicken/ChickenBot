        "bent": "^7.3.12",
        "canvas": "^2.8.0",
        "colorette": "^1.3.0",
        "discord.js": "^13.1.0",
        "dotenv": "^10.0.0",
        "enmap": "^5.8.7",
        "express": "^4.17.1",
        "express-rate-limit": "^5.3.0",
        "fluent-ffmpeg": "^2.1.2",
